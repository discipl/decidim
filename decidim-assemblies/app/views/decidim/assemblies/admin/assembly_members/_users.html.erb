<div class="picker-header">
  <h1><%= t(".current_selection") %></h1>
</div>
<div class="picker-content">
  <input type="text" name="users-search" data-url="<%= users_assembly_members_url(assembly_slug: @current_assembly.slug) %>" id="data_picker-autocomplete"/>
</div>
<div class="picker-footer">
  <div class="buttons button--double">
    <a id="user-picker-choose" class="button mute" href="#" data-picker-value="" data-picker-text="" ><%= t(".select") %></a>
    <a class="button clear" data-close=""><%= t(".close") %></a>
  </div>
</div>
